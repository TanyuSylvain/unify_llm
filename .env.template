# UnifyLLM API Configuration Template
# Copy this file to .env and add your API keys
# At least one API key is required for the application to work

# Mistral AI
# Get your API key from: https://console.mistral.ai/
MISTRAL_API_KEY=your_mistral_api_key_here

# Qwen (Alibaba DashScope)
# Get your API key from: https://dashscope.aliyuncs.com/
QWEN_API_KEY=your_qwen_api_key_here
QWEN_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1

# GLM (Zhipu AI)
# Get your API key from: https://open.bigmodel.cn/
GLM_API_KEY=your_glm_api_key_here
GLM_BASE_URL=https://open.bigmodel.cn/api/paas/v4

# MiniMax
# Get your API key from: https://www.minimaxi.com/
MINIMAX_API_KEY=your_minimax_api_key_here
MINIMAX_BASE_URL=https://api.minimax.io/v1

# DeepSeek
# Get your API key from: https://platform.deepseek.com/
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_BASE_URL=https://api.deepseek.com

# OpenAI-compatible API
# Can be OpenAI or any OpenAI-compatible service
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_BASE_URL=https://api.openai.com/v1

# Google Gemini
# Get your API key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_BASE_URL=https://generativelanguage.googleapis.com
